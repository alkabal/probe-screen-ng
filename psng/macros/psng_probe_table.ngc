o<psng_probe_table> sub
;o<psng_hook> call [3]

o<psng_config_check> call

o300 if [#<_current_tool> NE 1]
     o<psng_hook_end> call
     (ABORT,PROBING TABLE IS NOT ALLOWED WITHOUT 3D PROBE AS TOOL 1)
o300 endif


(cancel all Z offsets)
G92.1
G49
G10 L20 P0 Z[#<_hal[axis.z.pos-cmd]>]
#<z>=#<_z> (save current Z position)

o<psng_start_touch_probing> call

#<touch_result> = #5063
(print, probe Z=#<touch_result>)

(back to start point)
G1 Z#<z> F#<_ini[TOOLSENSOR]RAPID_SPEED>
G43

;o<psng_hook_end> call
o<psng_probe_table> endsub
M2
