o<psng_yplus> sub
o<psng_hook> call [9]

o100 if [#<_hal[probe.ps_searchvel]> LE 0]
o100 return [-1] ; indicate searchvel <= 0
o100 endif

o200 if [#<_hal[probe.ps_probevel]> LE 0]
o200 return [-2] ; indicate probevel <= 0
o200 endif

o300 if [#5400 EQ 0]
     (MSG,PROBE WITH TOOL 0 NOT ALLOWED)
     M2
o300 endif

#<y>=#<_y> (save start Y position) 
G91
G38.2 Y#<_hal[probe.ps_probe_max]>    F#<_hal[probe.ps_searchvel]>
G38.4 Y-#<_hal[probe.ps_probe_latch]> F#<_hal[probe.ps_searchvel]>     ;DO NOT WORK FINE WITHOUT SPRING MOUNTED PROBE AND SETTER
G1    Y-0.5                           F#<_ini[TOOLSENSOR]RAPID_SPEED>
G38.2 Y[#<_hal[probe.ps_probe_latch]>*2] F#<_hal[probe.ps_probevel]>
(print, probe Y=#5062 )
G90
(back to start point)
G1 Y#<y> F#<_ini[TOOLSENSOR]RAPID_SPEED> 
	
o<psng_hook_end> call 
o<psng_yplus> endsub
M2
