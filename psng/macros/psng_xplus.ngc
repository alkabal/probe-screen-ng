o<psng_xplus> sub

o10 if [#<_hal[motion.probe-input]> EQ 1]
     o<psng_hook_end> call
    (ABORT,!!! PROBE OR SETTER ALREADY TRIGGERED BEFORE PROBE X+ !!!)
o10 endif

#<x>=#<_x> (save start X position)
G91

G38.3 X#<_hal[probe.ps_probe_max]>          F#<_hal[probe.ps_searchvel]>
o500 call

  o100 if [#<_ini[PROBE_SCREEN]PROBE_REVERSE_LATCH> GT 0]
       G38.5 X-#<_ini[PROBE_SCREEN]PROBE_REVERSE_LATCH>       F#<_hal[probe.ps_searchvel]>     ;DO NOT WORK FINE WITHOUT SPRING MOUNTED PROBE
       o500 call
       G1    X-[#<_ini[PROBE_SCREEN]PROBE_LATCH>] F#<_hal[probe.ps_searchvel]>
  o100 else
       G1    X-[#<_hal[probe.ps_probe_latch]>] F#<_hal[probe.ps_searchvel]>
  o100 endif

G38.3 X[#<_hal[probe.ps_probe_latch]>*1.2]    F#<_hal[probe.ps_probevel]>
o500 call

(print,probe X=#5061)
G90
(back to start point)
G1 X#<x> F#<_ini[PROBE_SCREEN]RAPID_SPEED>

o500 sub
     o510 if [#5070 EQ 0]
          o<psng_hook_end> call
          (ABORT,FAILURE : PROBE X+ DOES NOT FOUND THE CONTACT POINT)
     o510 endif
o500 endsub

o<psng_xplus> endsub
M2
