o<psng_goto_toolsensor> sub

o10 if [#<_hal[motion.probe-input]> EQ 1]
     o<psng_hook_end> call
    (ABORT,!!! PROBE OR SETTER ALREADY TRIGGERED BEFORE GOTO TOOLSENSOR FOR AUTOLENGTH !!!)
o10 endif

;first go up
G90
o20 if [#<_ini[EMCIO]TOOL_CHANGE_QUILL_UP> EQ 1]
    G53 G0 Z0                                     ; Respecting the traditional linuxcnc behavior
o20 endif

G53 G0 X[#<_ini[TOOLSENSOR]X>] Y[#<_ini[TOOLSENSOR]Y>]
G53 G0 Z[#<_ini[TOOLSENSOR]Z>]

o<psng_goto_toolsensor> endsub
M2
